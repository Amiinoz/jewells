        "url": "https://github.com/Amiinoz/mohamed-boilerplate/issues"
    },
    "homepage": "https://github.com/Amiinoz/mohamed-boilerplate/blob/main/README.md",
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^6.3.0",
